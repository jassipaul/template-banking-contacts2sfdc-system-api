<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns:tls="http://www.mulesoft.org/schema/mule/tls"
	xmlns:apikit="http://www.mulesoft.org/schema/mule/mule-apikit"
	xmlns:salesforce="http://www.mulesoft.org/schema/mule/salesforce"
	xmlns:http="http://www.mulesoft.org/schema/mule/http" xmlns:ee="http://www.mulesoft.org/schema/mule/ee/core"
	xmlns="http://www.mulesoft.org/schema/mule/core" xmlns:doc="http://www.mulesoft.org/schema/mule/documentation"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
http://www.mulesoft.org/schema/mule/tls http://www.mulesoft.org/schema/mule/tls/current/mule-tls.xsd 
	http://www.mulesoft.org/schema/mule/mule-apikit http://www.mulesoft.org/schema/mule/mule-apikit/current/mule-apikit.xsd http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/current/mule.xsd
	http://www.mulesoft.org/schema/mule/http http://www.mulesoft.org/schema/mule/http/current/mule-http.xsd 
	http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd
	http://www.mulesoft.org/schema/mule/salesforce http://www.mulesoft.org/schema/mule/salesforce/current/mule-salesforce.xsd">

	<http:listener-config name="banking_contact_system_api-httpListenerConfig">
		<http:listener-connection host="0.0.0.0"
			port="${https.port}" tlsContext="TLS_Context" protocol="HTTPS">
		</http:listener-connection>
	</http:listener-config>
	<apikit:config name="banking_contact_system_api-config"
		raml="banking_contact_system_api.raml" outboundHeadersMapName="outboundHeaders"
		httpStatusVarName="httpStatus" />


	<salesforce:sfdc-config name="Salesforce_Config"
		doc:name="Salesforce Config" doc:id="dedd28e9-54c1-4fa1-9dc4-9abdfbc6c010">
		<salesforce:basic-connection username="${sfdc.username}"
			password="${sfdc.password}" securityToken="${sfdc.securityToken}" />
	</salesforce:sfdc-config>
	<tls:context name="TLS_Context" doc:name="TLS Context"
		doc:id="1a87df8b-3531-40b4-8932-2f1918a6a9d6">
		<tls:key-store type="jks" path="${keystore.location}"
			alias="${key.alias}" keyPassword="${keystore.password}" password="${key.password}" />
	</tls:context>
	<configuration-properties doc:name="Configuration properties"
		doc:id="3f8e1fb3-5d4e-4016-a78a-5dc6d213111d" file="mule.${mule.env}.properties" />
	<configuration-properties doc:name="Configuration properties"
		doc:id="7a61e8bb-6e38-4be7-b8a7-c9e552ef8db1" file="common.properties" />
	<global-property doc:name="Global Property" name="mule.env"
		value="dev" />

</mule>
